        });

        $("#overheadTank area:eq(1)").click(function () {
            alert("123456");
        });

        var infopanel = document.getElementById("side_div");

        var map = document.getElementById("layout");
        var svgdoc;

        map.addEventListener("load", function () {
            svgdoc = map.contentDocument;

            updateinfo(svgdoc);

            var shal = svgdoc.getElementById("shal");
            var ritu = svgdoc.getElementById("ritu");
            var kolarps = svgdoc.getElementById("kolarps");
            var raj = svgdoc.getElementById("raj");
            var bairagarh = svgdoc.getElementById("bairagarh");

            shal.addEventListener("mousedown", function () {
                infopanel.style.display = "block";
                $("#name_oht").text("Shalimar");
                $("#hide_side_div").click(function () {
                    infopanel.style.display = "none";
                });
            }, false);

        }, false);

        setInterval(updateinfo, 5000);
        function updateinfo() {
            console.log(new Date().toLocaleTimeString().split(":")[0]);
            var shalimarCap = svgdoc.getElementById("shalimar_cap");
            var sCap = <%=shalimar_storage%> + " KL";
            var update = new Date().toDateString().slice(4) + ", " + new Date().toLocaleTimeString().split(":")[0] + ":" + new Date().toLocaleTimeString().split(":")[1] + " " + new Date().toLocaleTimeString().split(" ")[1];

            shalimarCap.childNodes[0].textContent = sCap;

            svgdoc.getElementById("shal_storage").textContent = getRandomInt(9);

            svgdoc.getElementById("shal_lastUpdate").textContent = update;
            svgdoc.getElementById("ritu_update").textContent = update;
            svgdoc.getElementById("kolar_update").textContent = update;
            svgdoc.getElementById("raj_update").textContent = update;
            svgdoc.getElementById("bair_update").textContent = update;

        }

        function getRandomInt(max) {
            return Math.floor(Math.random() * Math.floor(max));
        }
    });
</script>

